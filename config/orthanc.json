{
  "Name": "Orthanc",
  "StorageDirectory": "/var/lib/orthanc/db",
  "RegisteredUsers": {
    "orthanc": "orthanc"
  },
  "DicomAet": "ORTHANC",
  "DicomPort": 4242,
  "HttpPort": 8042,

  "Plugins": [
      "/usr/share/orthanc/plugins/libOrthancWebViewer.so",
      "/usr/share/orthanc/plugins/libOrthancDicomWeb.so",
      "/usr/share/orthanc/plugins/libModalityWorklists.so",
      "/usr/share/orthanc/plugins/libOrthancExplorer2.so",
      "/usr/share/orthanc/plugins/libOrthancGdcm.so"
    ],
  
  "DicomWeb": {
    "Enable": true,
    "Root": "/dicom-web/",
    "EnableWado": true,
    "WadoRoot": "/wado/",
    "Host": "0.0.0.0",
    "Port": 8042,
    "Ssl": false
  },

  "WebViewer": {
    "CachePath": "/var/lib/orthanc/WebViewerCache",
    "CacheSize": 100,
    "EnableGdcm": true,
    "RestrictOrigins": false,
    "Threads": 4
  },
  
  "HttpsCACertificates": "/etc/ssl/certs/ca-certificates.crt",
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": false,
  "SslEnabled": false,
  
  "HttpHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization, Cache-Control"
  },
  
  "Worklists": {
    "Enable": true,
    "Database": "/worklists"
  },
  "DicomModalityWorklists": {
    "Enable": true,
    "CheckModality": false,
    "WorklistsFolder": "/worklists"
  },
  "DicomModalities": {
    "WORKSTATION": [
      "WORKSTATION",
      "127.0.0.1",
      11112
    ],
    "TESTMODALITY": [
      "TESTMODALITY", 
      "localhost",
      4242
    ],
    "FINDSCU": [
      "FINDSCU",
      "172.17.0.1",
      104
    ]
  }
}