{
  "Name": "Orthanc",
  "StorageDirectory": "/var/lib/orthanc/db",
  "RegisteredUsers": {
    "orthanc": "orthanc"
  },
  "Plugins": [
    "/usr/share/orthanc/plugins"
  ],
  "DicomAet": "ORTHANC",
  "DicomPort": 4242,
  "HttpPort": 8042,
  
  "DicomWeb": {
    "Enable": true,
    "Root": "/dicom-web/",
    "EnableWado": true,
    "WadoRoot": "/wado/",
    "Host": "0.0.0.0",
    "Port": 8042,
    "Ssl": false
  },
  
  "HttpsCACertificates": "/etc/ssl/certs/ca-certificates.crt",
  "RemoteAccessAllowed": true,
  "AuthenticationEnabled": false,
  "SslEnabled": false,
  
  "HttpHeaders": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
    "Access-Control-Allow-Headers": "Content-Type, Authorization, Cache-Control"
  },
  
  "Worklists": {
    "Enable": true,
    "Database": "/worklists"
  },
  "DicomModalityWorklists": {
    "Enable": true,
    "CheckModality": false,
    "WorklistsFolder": "/worklists"
  },
  "DicomModalities": {
    "FINDSCU": [
      "FINDSCU",
      "192.168.65.1",
      104
    ]
  }
}